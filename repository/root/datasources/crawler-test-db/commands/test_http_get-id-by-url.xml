<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE command SYSTEM "../../../../dtds/script-dao-commands.dtd">
<command name="test.http.get-id-by-url" type="SQL.QUERY">
         <parameters>
                     <parameter name="url" />
               </parameters>
      <script>
      select id from 
pending_urls 
where url='$url'
   </script>
</command>