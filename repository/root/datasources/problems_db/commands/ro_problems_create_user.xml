<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE command SYSTEM "../../../../dtds/script-dao-commands.dtd">
<command name="ro.problems.create_user" type="SQL.UPDATE">
         <parameters>
                     <parameter name="username" />
                     <parameter name="password" />
                     <parameter name="email" />
               </parameters>
      <script>
      insert into users(username,password,email,user_hash) values('$username','$password','$email',md5('$username'||'$password'||'$email'))

   </script>
</command>