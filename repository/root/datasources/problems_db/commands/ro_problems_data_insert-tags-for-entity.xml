<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE command SYSTEM "../../../../dtds/script-dao-commands.dtd">
<command name="ro.problems.data.insert-tags-for-entity" type="SQL.UPDATE">
         <parameters>
                     <parameter name="entityId" />
                     <parameter name="tagsIds" />
               </parameters>
      <script>
      #foreach($row in $tagsIds.getValues())
insert into entities_tags(entity_id,tag_id) values($entityId,$row.getValue("id"));
#end
   </script>
</command>