<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entities SYSTEM "../../dtds/entities.dtd">
<entities>
   <entity id="test.testEmbededFlow" type="workflow-definition"
      logic-type="dynamic-wrapper">
      <entity type="description">
         <value>This is how a workflow will look like</value>
      </entity>
      <entity logic-type="config">
         <entity type="definition-params" logic-type="static-parameters">
            <value>
               <map>
                  <entry key="inputParameters">
                     <value>
                        <list>
                           <value>
                              <entity type="input-parameter">
                                 <property name="name" value="enteredValue"></property>
                                 <property name="type" value="Number"></property>
                                 <property name="mandatory"
                                    value="true"></property>
                              </entity>
                           </value>
                        </list>
                     </value>
                  </entry>
                  <entry key="executionEntity">
                     <value>
                        <entity type="workflow" logic-type="dynamic-wrapper">
                           <entity logic-type="config">
                              <entity logic-type="executor">
                                 <entity-ref ref-id="workFlow" />
                              </entity>
                              <entity logic-type="original-input-name">
                                 <value>fullInputContext</value>
                              </entity>
                              <entity type="workflow-params"
                                 logic-type="static-parameters">
                                 <value>
                                    <map>
                                       <entry key="defaultEntryPointId"
                                          value="comp1">
                                       </entry>
                                       <entry key="components">
                                          <value>
                                             <map>
                                                <entry key="comp1">
                                                   <value>
                                                      <entity
                                                         type="workflow-component">
                                                         <entity
                                                            type="component"
                                                            logic-type="component">
                                                            <entity
                                                               logic-type="dynamic-wrapper">
                                                               <entity
                                                                  logic-type="config">
                                                                  <entity
                                                                     logic-type="executor">
                                                                     <entity-ref
                                                                        ref-id="core.logic.embeded-flow" />
                                                                  </entity>
                                                                  <entity
                                                                     logic-type="original-input-name">
                                                                     <value>argsContext</value>
                                                                  </entity>
                                                                  <entity
                                                                     logic-type="static-parameters">
                                                                     <value>
                                                                        <map>
                                                                           <entry key="fid">
                                                                              <value-type value="generatedFlow2" type="String" />
                                                                           </entry>
                                                                        </map>
                                                                     </value>
                                                                  </entity>
                                                                  <entity
                                                                     logic-type="dynamic-parameters">
                                                                     <value>
                                                                        <map>
                                                                           <entry key="enteredValue" value="enteredValue" />
                                                                        </map>
                                                                     </value>
                                                                  </entity>
                                                               </entity>
                                                            </entity>
                                                         </entity>
                                                         <entity
                                                            logic-type="output-params-mappings">
                                                            <value>
                                                               <map>
                                                                  <entry
                                                                     key="exit"
                                                                     value="innerExit" />
                                                               </map>
                                                            </value>
                                                         </entity>
                                                         <entity
                                                            type="flow-logic"
                                                            logic-type="flow-logic">
                                                            <property
                                                               name="exitLabel"
                                                               value="done" />
                                                         </entity>
                                                      </entity>
                                                   </value>
                                                </entry>
                                             </map>
                                          </value>
                                       </entry>
                                    </map>
                                 </value>
                              </entity>
                              <entity logic-type="dynamic-parameters">
                                 <value>
                                    <map>
                                       <entry key="inputContext"
                                          value="origContext"></entry>
                                       <entry key="entryPointId"
                                          value="entryPointId"></entry>
                                       <entry key="isAsyncContext"
                                          value="isAsyncContext"></entry>
                                    </map>
                                 </value>
                              </entity>
                           </entity>
                        </entity>
                     </value>
                  </entry>
                  <entry key="outputParameters">
                     <value>
                        <list>
                           <value>
                              <entity type="output-parameter">
                                 <property name="name" value="exit"></property>
                              </entity>
                           </value>
                           <value>
                              <entity type="output-parameter">
                                 <property name="name" value="innerExit"></property>
                              </entity>
                           </value>
                        </list>
                     </value>
                  </entry>
               </map>
            </value>
         </entity>
         <entity logic-type="original-input-name">
            <value>inputContext</value>
         </entity>
         <entity logic-type="executor">
            <entity-ref ref-id="workflowAdapterEntity" />
         </entity>
      </entity>
   </entity>
</entities>